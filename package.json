{
  "name": "portfolio_front",
  "version": "1.0.0",
  "description": "My personal portfolio website Frontend. using MEVN stack.",
  "main": "server.js",
  "scripts": {
    "start:server": "node server.js --ignore './src/'",
    "start:src": "npm run dev --prefix './portfolio'",
    "stop:server": "pkill --signal SIGINT myapp",
    "dev": "concurrently \"npm run start:server\" \"npm run start:src\""
  },
